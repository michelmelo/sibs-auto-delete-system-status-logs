{
    "name": "michelmelo/woocommerce-logs",
    "type": "wordpress-plugin",
    "homepage": "http://michelmelo.pt",
    "license": "proprietary",
    "description": "View and delete logs  for WooCommerce",
    "keywords": ["plugin"],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14"
    },
    "archive": {
        "exclude": [
            ".*",
            "!.gitignore",
            "node_modules/",
            "vendor/",
            "www/",
            "composer/",
            "*.lst",
            "*.zip",
            "composer.json",
            "composer.lock",
            "*.yml"
        ]
    },
    "scripts": {
        "archive": [
            "@composer archive -f zip --file woocommerce-logs"
        ],
        "fixer": [
            "php-cs-fixer fix"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true

}